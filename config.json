                    "template": "gerkindevelopment",
                    "typepath": "javascript"
                },
                "ithoughts-index-doc": {
                    "template": "ithoughts-index-doc",
                    "head_links": {
                        "GerkinDevelopment": "https://www.gerkindevelopment.net/",
