                    },
                    "doc_links": [
                        {
                            "labels": {
                                "main": "JavaScript",
                                "sub": "Node.js"
                            },
                            "image": "/images/serverside.jpg",
                            "link": "/javascript/index.html"
                        }
                    ]
                },
                "version-history": {
                    "template": "ithoughts-index-doc"
                }
            },
            [
                {
                    "git": {
                        "actions": [
                            {
                                "action": "commit",
                                "data": {
                                    "message": "Release version %VERSION%"
                                }
                            },
                            {
                                "action": "tag",
                                "data": {
                                    "label": "v%VERSION%",
                                    "message": "Release version %VERSION%"
                                }
                            },
                            {
                                "action": "push"
                            }
                        ]
                    }
                }
            ]
