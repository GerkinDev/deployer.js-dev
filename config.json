        },
        "commands": [
            {
                "git": {
                    "actions": [
                        {
                            "action": "commit",
                            "data": {
                                "message": "Pre-process release v%VERSION%"
                            }
                        }
                    ]
                }
            },
            {
                "files-version": {
                    "selection": {
                        "\\.(js)$": true,
                        "\\.min\\.(js)$": false,
                        "^\\.\\/node_modules($|\\/)": false,
                        "^\\.\\/templates($|\\/)": false
                    },
                    "setDiffs": {
                        "/(@version\\s+)\\d+(\\.\\d+)*": "$1%VERSION%"
                    }
                }
            },
            {
                "jsdoc": {
                    "private": true,
                    "selection": {
                        "\\.(js)$": true,
                        "\\.min\\.(js)$": false,
                        "^\\.\\/node_modules($|\\/)": false,
                        "^\\.\\/templates($|\\/)": false,
                        "^\\.\\/actions\\/ithoughts-index-doc\\.js": false
                    },
                    "template": "gerkindevelopment",
                    "typepath": "javascript"
                },
                "ithoughts-index-doc": {
                    "template": "ithoughts-index-doc",
                    "head_links": {
                        "GerkinDevelopment": "https://www.gerkindevelopment.net/",
