            }
        },
        "company": {
            "start": "2015",
            "name": "GerkinDevelopment"
        },
        "commands": [
            {
                "git": {
                    "actions": [
                        {
                            "action": "commit",
                            "data": {
                                "message": "Pre-process release v%VERSION%"
                            }
                        }
                    ]
                }
            },
            {
                "files-version": {
                    "selection": {
                        "\\.(js)$": true,
                        "\\.min\\.(js)$": false,
                        "^\\.\\/node_modules($|\\/)": false,
                        "^\\.\\/templates($|\\/)": false
                    },
                    "setDiffs": {
                        "/(@version\\s+)\\d+(\\.\\d+)*": "$1%VERSION%"
                    }
                }
            },
            {
                "jsdoc": {
                    "private": true,
                    "selection": {
                        "\\.(js)$": true,
                        "\\.min\\.(js)$": false,
                        "^\\.\\/node_modules($|\\/)": false,
                        "^\\.\\/templates($|\\/)": false,
                        "^\\.\\/actions\\/ithoughts-index-doc\\.js": false
